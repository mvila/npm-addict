{
  "name": "1place-integration-npm",
  "version": "0.1.0",
  "description": "npm integration for 1Place",
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "private": true,
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/kinda/1place-integration-npm"
  },
  "scripts": {
    "compile": "babel src --out-dir lib",
    "prepublish": "npm run compile",
    "release": "npm run lint && npm run compile && npm test && (git checkout --quiet master && test -z \"$(git status --porcelain)\" && npm version $npm_config_release_type && git push --follow-tags) || echo \"An error occured (unclean repository?)\"",
    "release-patch": "npm run release --release-type=patch",
    "release-minor": "npm run release --release-type=minor",
    "release-major": "npm run release --release-type=major",
    "lint": "eslint .",
    "start": "babel-node src/index.js",
    "test": "mocha --compilers js:babel-register,js:babel-polyfill"
  },
  "dependencies": {
    "better-clone": "^1.0.3",
    "event-emitter-mixin": "^0.3.1",
    "github-url-to-object": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "object-layer": "^0.4.1",
    "sleep-promise": "^1.0.0",
    "universal-log": "^0.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-transform-async-to-generator": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "eslint": "^2.0.0-beta.1",
    "eslint-config-next": "^0.1.0",
    "mocha": "^2.3.4"
  }
}
